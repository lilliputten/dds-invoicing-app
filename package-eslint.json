{
  "eslint-unused-rules": {
    "eqeqeq": [
      "warn",
      "smart"
    ],
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-debugger": "warn",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ],
      "prettier/prettier": [
        "warn",
        {
          "endOfLine": "auto"
        }
      ]
    }
  }
}
